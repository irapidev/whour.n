{
  "name": "whour",
  "version": "0.0.0",
  "description": "Who You Are, is an IP to location service.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "git:commit": "git-cz",
    "test:types": "tsc --noEmit",
    "test:types:watch": "npm run test:types -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build": "npm run build:types && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidevelop/whour.git"
  },
  "keywords": [],
  "author": "Iman Vafadarnezhad",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/rapidevelop/whour/issues"
  },
  "homepage": "https://github.com/rapidevelop/whour#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/node": "^11.9.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "typescript": "^3.3.3"
  }
}
